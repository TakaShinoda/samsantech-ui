{
  "name": "samsantech-ui",
  "version": "0.1.0",
  "repository": "https://github.com/TakaShinoda/samsantech-ui.git",
  "scripts": {
    "dev": "vite",
    "tsc": "vue-tsc --noEmit",
    "build": "vue-tsc --noEmit && npm run test && vite build",
    "serve": "vite preview",
    "test": "jest"
  },
  "types": "src/index.ts",
  "module": "./dist/samsantech-ui.es.js",
  "main": "./dist/samsantech-ui.umd.js",
  "files": [
    "dist",
    "src/index.ts",
    "src/components/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/samsantech-ui.es.js",
      "require": "./dist/samsantech-ui.umd.js"
    },
    "./style": "./dist/style.css"
  },
  "peerDependencies": {
    "vue": "^3.0.5",
    "@vue/compiler-sfc": "^3.0.5"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "vue": "^3.2.6"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/test-utils": "2.0.0-rc.6",
    "autoprefixer": "^10.3.4",
    "babel-jest": "^26.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.17.0",
    "jest": "^26.0.0",
    "msw": "^0.35.0",
    "postcss": "^8.3.6",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.9",
    "tailwindcss-pseudo-elements": "^2.0.0",
    "ts-jest": "^26.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.5.2",
    "vue-jest": "5.0.0-alpha.10",
    "vue-tsc": "^0.2.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
